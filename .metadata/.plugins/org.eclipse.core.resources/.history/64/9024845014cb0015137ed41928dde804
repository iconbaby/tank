import java.awt.Color;
import java.awt.Graphics;


public class Missile {
	int x,y;
	Tank.Direction dir;
	public Missile(int x,int y,Tank.Direction dir) {
		this.x = x;
		this.y = y;
		this.dir = dir;
	}
	
	public void draw(Graphics g) {
		Color c = g.getColor();
		g.setColor(Color.black);
		g.fillOval(x, y, 10, 10);
		g.setColor(c);
		move();
	}

}